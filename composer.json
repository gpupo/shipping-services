{
    "name": "gpupo/shipping-services",
    "description": "Acesso a informações de pacotes remetidos no Brasil",
    "homepage": "https://opensource.gpupo.com/shipping-services/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["correios", "sdk", "api"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-soap" : "*",
        "gpupo/common-sdk": "^4.5|dev-master",
        "zendframework/zend-soap": "^2.6"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.2|dev-master",
      "symfony/var-dumper": "^4.1|dev-master"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\ShippingServices\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\ShippingServices\\": "tests"
        }
    },
    "bin": ["bin/shipping-services"],
    "scripts": {
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
